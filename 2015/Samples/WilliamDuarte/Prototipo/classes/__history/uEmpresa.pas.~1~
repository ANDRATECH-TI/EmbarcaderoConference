unit uEmpresa;

interface

Type
  TEmpresa = class
    property RazaoSocial  : String;
    property NomeFantasia : String;
    property CNPJ : String;
    property IE : String;
    property IM : String;
    property Endereco : String;
    property Numero : String;
    property Bairro : String;
    property CEP : String;
    property Complemento : String;
    property UF : String;
    property CodigoUF : Integer;
    property Municipio : String;
    property CodigoMunicipio : Integer;
    property Email : String;
    property Telefone : String;
  end;

implementation

end.
