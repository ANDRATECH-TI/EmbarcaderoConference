unit uCNotaFiscalEletronica;

interface

uses
  System.SysUtils,
  System.Variants,
  System.Classes,
  ACBrNFe;

type
  TNotaFiscal = class
  public
    function EnviaNFe: string;
  end;

implementation

{ TNotaFiscal }

function TNotaFiscal.EnviaNFe: string;
var
  lACBrNFe: TACBrNFe;
begin
  lACBrNFe := TACBrNFe.Create(nil);
  lACBrNFe.Configuracoes.Certificados.NumeroSerie := '0EA583324600E3FB';
  lACBrNFe.

end;

end.
